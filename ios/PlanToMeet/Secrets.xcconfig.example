// Secrets.xcconfig.example
// Copy this file to Secrets.xcconfig and fill in your values.
// DO NOT commit Secrets.xcconfig to source control.

SUPABASE_URL = https:/$()/your-project.supabase.co/rest/v1
SUPABASE_ANON_KEY = your-anon-key-here

// Sentry (Error Monitoring — https://sentry.io)
// Get DSN from: Sentry Dashboard → Project Settings → Client Keys (DSN)
// Leave empty to disable Sentry in this build
SENTRY_DSN =
SENTRY_ENVIRONMENT = production
