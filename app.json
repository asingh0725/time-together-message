{
  "expo": {
    "name": "PlanToMeet",
    "slug": "plantomeet",
    "scheme": "plantomeet",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aviraj.plantomeet",
      "associatedDomains": [
        "applinks:plantomeet.app",
        "webcredentials:plantomeet.app"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.aviraj.plantomeet",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "plantomeet.app",
              "pathPrefix": "/poll"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://plantomeet.app"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
